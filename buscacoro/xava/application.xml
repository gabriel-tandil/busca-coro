<application name="buscacoro">

	<!-- A default module for each entity is assumed with the controllers on 
		<default-module/> -->

	<default-module>
		<controller name="Typical" />
		<controller name="FiltroSimple" />
	</default-module>

	<module name="ConsultaCoro">
		<model name="Coro" />
		<view name="consulta" />
		<tab name="consulta" />
		<env-var name="XAVA_SEARCH_ACTION" value="CRUD.searchReadOnly" />
		<controller name="Print" />
	</module>

	<module name="ConsultaRecurso">
		<model name="Recurso" />
		<view name="consulta" />
		<tab name="consulta" />
		<env-var name="XAVA_SEARCH_ACTION" value="CRUD.searchReadOnly" />
		<controller name="Print" />
	</module>


</application>

